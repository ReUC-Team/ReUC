{
  "post": {
    "summary": "(Web) Create a Team for a Project",
    "description": "Creates a new team for a project by adding multiple members in a batch.\n\n- Requires **\"professor\"** role.\n\n**Note:** If a user is submitted in the `members` array who is already on the team, they will be silently skipped. This endpoint will not return a 409 Conflict error for duplicate entries.",
    "tags": ["Project"],
    "security": [{ "BearerAuth": [], "CsrfAuth": [] }],
    "parameters": [
      {
        "name": "uuid",
        "in": "path",
        "description": "The unique identifier (UUID) of the project.",
        "required": true,
        "schema": {
          "type": "string",
          "format": "uuid",
          "example": "a1b2c3d4-e5f6-7890-a1b2-c3d4e5f67890"
        }
      }
    ],
    "requestBody": {
      "required": true,
      "content": {
        "application/json": {
          "schema": {
            "$ref": "../payload/TeamProjectCreate.json"
          }
        }
      }
    },
    "responses": {
      "201": {
        "description": "Team created successfully.",
        "content": {
          "application/json": {
            "schema": { "$ref": "../responses/CreateTeamProjectSuccess.json" }
          }
        }
      },
      "400": { "$ref": "../../../responses/Error400.json" },
      "401": { "$ref": "../../../responses/Error401.json" },
      "403": { "$ref": "../../../responses/Error403.json" },
      "404": { "$ref": "../../../responses/Error404.json" },
      "500": { "$ref": "../../../responses/Error500.json" }
    }
  }
}
